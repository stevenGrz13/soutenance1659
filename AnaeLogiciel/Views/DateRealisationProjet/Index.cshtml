@model IEnumerable<AnaeLogiciel.Models.DateRealisationProjet>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutGlobalProject";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateDebutRealisation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateFinRealisation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Projet)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateDebutRealisation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateFinRealisation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Projet.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
